{
  "source": {
    "include": [
      "./services/",
      "./utils/",
      "./mcp/",
      "./api/",
      "./toolm/",
      "./index.js"
    ],
    "exclude": [
      "node_modules/",
      "dist/",
      "coverage/",
      "tests/",
      "test-results/"
    ],
    "includePattern": ".+\\.js(doc)?$"
  },
  "opts": {
    "destination": "./docs/jsdoc/",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}